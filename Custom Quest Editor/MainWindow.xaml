                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <ComboBox Grid.Column="0" Name="QuestObjectiveIcon1"/>
                                        <ComboBox Grid.Column="2" Name="QuestObjectiveIcon2"/>
                                        <ComboBox Grid.Column="4" Name="QuestObjectiveIcon3"/>
                                        <ComboBox Grid.Column="6" Name="QuestObjectiveIcon4"/>
                                        <ComboBox Grid.Column="8" Name="QuestObjectiveIcon5"/>
                                    </Grid>
                                </GroupBox>
                            </StackPanel>
                        </ScrollViewer>
                    </TabItem>
                    <TabItem Header="Objectives">
