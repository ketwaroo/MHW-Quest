<Window x:Class="Custom_Quest_Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Custom_Quest_Editor"
        mc:Ignorable="d"
        Title="Monster Hunter World Quest Editor" Height="800" Width="800">
    <Grid>
        <Grid.Resources>
            <Style TargetType="TextBox">
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="Text" Value="0"/>
            </Style>
            <Style TargetType="ComboBox">
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="SelectedIndex" Value="0"/>
            </Style>
            <Style TargetType="Label">
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
        </Grid.Resources>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="File">
                    <MenuItem Header="Open..." Click="Open_Click"/>
                    <MenuItem Header="Save" Name="SaveM" IsEnabled="False" Click="Save_Click"/>
                    <MenuItem Header="Save As" Name="SaveAs" IsEnabled="False" Click="SaveAs_Click"/>
                    <Separator/>
                    <MenuItem Header="Import" Name="Import" IsEnabled="False" Click="Import_Click" />
                    <MenuItem Header="Export" Name="Export" IsEnabled="False" Click="Export_Click" />
                    <Separator/>
                    <MenuItem Header="Exit" Click="Exit_Click"/>
                </MenuItem>
                <MenuItem Header="Testing">
                    <MenuItem Header="Enable Editing Unknowns" IsCheckable="True" Checked="UnknownsChecked" Unchecked="UnkownsUnchecked"/>
                </MenuItem>
                <MenuItem Header="Help">
                    <MenuItem Header="About" Click="About_Click"/>
                    <MenuItem Header="Contact" Click="Contact_Click"/>
                </MenuItem>
            </Menu>
            <Border Grid.Row="1" Padding="10" BorderBrush="Black">
                <TabControl>
                    <TabItem Header="Common">
                        <ScrollViewer VerticalScrollBarVisibility="Auto">
                            <StackPanel>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="85"/>
                                        <ColumnDefinition Width="33"/>
                                        <ColumnDefinition Width="20"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="50"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="20"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <Label Content="Quest ID" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <TextBox PreviewTextInput="NumberValidationTextBox" MaxLength="6" Grid.Column="3" Name="QuestID" Padding="0,5" Margin="0,5"/>
                                    <Label Grid.Column="5" Content="Stars" Margin="0,6"/>
                                    <ComboBox Grid.Column="7" Name="QuestStars" Padding="0,5" Margin="0,5" SelectionChanged="QuestRankSelectionChanged"/>
                                    <Label Grid.Row="1" Grid.Column="0" Content="Rank" Grid.ColumnSpan="2" Margin="0,5"/>
                                    <Label Grid.Row="1" Grid.Column="5" Content="Map" Margin="0,5"/>
                                    <Label Grid.Row="2" Grid.Column="0" Content="Player Spawn" Grid.ColumnSpan="2" Margin="0,5"/>
                                    <CheckBox Grid.Row="2" Grid.Column="5" Content="Fixed Spawn" Name="FixedSpawnFlag" Checked="FixedSpawnChanged" Unchecked="FixedSpawnChanged"/>
                                    <Label Grid.Row="3" Grid.Column="0" Content="Time Cycle" Grid.ColumnSpan="2" Margin="0,5"/>
                                    <Label Grid.Row="3" Grid.Column="5" Content="Weather Condition" Margin="0,5"/>
                                    <Label Grid.Row="4" Grid.Column="0" Content="Multiplayer Modifier" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <Label Grid.Row="4" Grid.Column="5" Content="Quest Timer" Margin="0,6"/>
                                    <Label Grid.Row="5" Grid.Column="0" Content="Reward Money" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <Label Grid.Row="5" Grid.Column="5" Content="Faint Penalty" Margin="0,6"/>
                                    <Label Grid.Row="6" Grid.Column="0" Content="HR Requirement" Grid.ColumnSpan="2" Margin="0,5"/>
                                    <Label Grid.Row="6" Grid.Column="5" Content="Background Music" Margin="0,5"/>
                                    <Label Grid.Row="7" Grid.Column="0" Content="Quest Type" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <Label Grid.Row="7" Grid.Column="5" Content="Hunter Rank Points" Margin="0,6"/>
                                    <Label Grid.Row="8" Grid.Column="0" Content="Reward rem ID" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <Label Grid.Row="9" Grid.Column="0" Content="Supply rem ID" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <Label Grid.Row="10" Grid.Column="0" Content="Number of Players" Grid.ColumnSpan="2" Margin="0,5"/>
                                    <Label Grid.Row="11" Grid.Column="0" Content="Sub1 Reward rem ID" Grid.ColumnSpan="2" Margin="0,6"/>
                                    <Label Grid.Row="11" Grid.Column="5" Content="Sub2 Reward rem ID" Margin="0,6"/>
                                    <Label Grid.Row="12" Grid.Column="0" Content="Quest Clear Music" Grid.ColumnSpan="2" Margin="0,5"/>
                                    <CheckBox Grid.Row="8" Grid.Column="5" Content="Map Toggle" Name="QuestMapToggleFlag"/>
                                    <CheckBox Grid.Row="9" Grid.Column="5" Content="Arch-Tempered Quest" Name="QuestArchTemperedFlag"/>
                                    <CheckBox Grid.Row="10" Grid.Column="5" Content="Pre-Selected Gear" Name="QuestPreSelectedGearFlag"/>
                                    <ComboBox Grid.Row="1" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestRank" SelectionChanged="QuestRankSelectionChanged"/>
                                    <ComboBox Grid.Row="1" Grid.Column="7" Padding="0,5" Margin="0,5" Name="QuestMap"/>
                                    <ComboBox Grid.Row="2" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestPlayerSpawnLocation" SelectionChanged="QuestPlayerSpawnLocationChanged"/>
                                    <ComboBox Grid.Row="3" Grid.Column="3" Padding="0,5" Margin="0,5" Name="TimeCycle"/>
                                    <ComboBox Grid.Row="3" Grid.Column="7" Padding="0,5" Margin="0,5" Name="WeatherCondition"/>
                                    <TextBox MaxLength="3" PreviewTextInput="NumberValidationTextBox" Grid.Row="4" Grid.Column="3" Padding="0,5" Margin="0,5" Name="MultiPlayerModifier" />
                                    <ComboBox Grid.Row="6" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestHunterHankRequirements" SelectionChanged="QuestRankSelectionChanged"/>
                                    <ComboBox Grid.Row="6" Grid.Column="7" Padding="0,5" Margin="0,5" Name="QuestBackgroundMusic"/>
                                    <ComboBox Grid.Row="7" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestType"/>
                                    <TextBox MaxLength="10" PreviewTextInput="NumberValidationTextBox" Grid.Row="4" Grid.Column="7" Padding="0,5" Margin="0,5" Name="QuestTimer"/>
                                    <TextBox MaxLength="10" PreviewTextInput="NumberValidationTextBox" Grid.Row="5" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestRewardMoney"/>
                                    <TextBox MaxLength="10" PreviewTextInput="NumberValidationTextBox" Grid.Row="5" Grid.Column="7" Padding="0,5" Margin="0,5" Name="QuestFaintPenalty"/>
                                    <TextBox MaxLength="10" PreviewTextInput="NumberValidationTextBox" Grid.Row="7" Grid.Column="7" Padding="0,5" Margin="0,5" Name="QuestHunterRankRewardPoints"/>
                                    <TextBox MaxLength="9" PreviewTextInput="NumberValidationTextBox" Grid.Row="8" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestItemRewardsRemID"/>
                                    <TextBox MaxLength="9" PreviewTextInput="NumberValidationTextBox" Grid.Row="9" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestSupplyBoxRemID"/>
                                    <TextBox MaxLength="9" PreviewTextInput="NumberValidationTextBox" Grid.Row="11" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestSub1RewardsRemID"/>
                                    <TextBox MaxLength="9" PreviewTextInput="NumberValidationTextBox" Grid.Row="11" Grid.Column="7" Padding="0,5" Margin="0,5" Name="QuestSub2RewardsRemID"/>
                                    <ComboBox Grid.Row="10" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestNumberOfPlayers"/>
                                    <ComboBox Grid.Row="12" Grid.Column="3" Padding="0,5" Margin="0,5" Name="QuestClearMusic"/>
                                </Grid>
