                                        <ComboBox Grid.Row="2" Grid.Column="2" Name="MainObjective2ID"/>
                                        <TextBox PreviewTextInput="NumberValidationTextBox" Grid.Row="4" Grid.Column="2" Name="MainObjective2Amount"/>
                                    </Grid>
                                </GroupBox>
                            </Grid>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <GroupBox Header="Sub Objective 1">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="010"/>
                                            <RowDefinition Height="auto"/>
                                        </Grid.RowDefinitions>
                                        <Label Grid.Row="0" Content="Objective Type:"/>
                                        <Label Grid.Row="2" Content="Objective ID:"/>
                                        <Label Grid.Row="4" Content="Amount"/>
                                        <CheckBox Grid.Row="1" Content="Multi-Monster" Name="SubObjective1MultiMonsterFlag"/>
                                        <ComboBox Grid.Column="2" Name="SubObjective1Type" SelectionChanged="SubObjective1Type_Changed"/>
                                        <ComboBox Grid.Row="2" Grid.Column="2" Name="SubObjective1ID"/>
                                        <TextBox PreviewTextInput="NumberValidationTextBox" Grid.Row="4" Grid.Column="2" Name="SubObjective1Amount"/>
                                    </Grid>
                                </GroupBox>
                                <GroupBox Grid.Column="2" Header="Sub Objective 2">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="010"/>
                                            <RowDefinition Height="auto"/>
                                        </Grid.RowDefinitions>
                                        <Label Grid.Row="0" Content="Objective Type:"/>
                                        <Label Grid.Row="2" Content="Objective ID:"/>
                                        <Label Grid.Row="4" Content="Amount"/>
                                        <CheckBox Grid.Row="1" Content="Multi-Monster" Name="SubObjective2MultiMonsterFlag"/>
                                        <ComboBox Grid.Column="2" Name="SubObjective2Type" SelectionChanged="SubObjective2Type_Changed"/>
                                        <ComboBox Grid.Row="2" Grid.Column="2" Name="SubObjective2ID"/>
                                        <TextBox PreviewTextInput="NumberValidationTextBox" Grid.Row="4" Grid.Column="2" Name="SubObjective2Amount"/>
                                    </Grid>
                                </GroupBox>
                            </Grid>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Monsters">
                        <TabControl>
                            <TabItem Header="Monster 1">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="20"/>
